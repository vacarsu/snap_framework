<%= graph font: :roboto, font_size: 20 %>

<%= primitive Scenic.Primitive.RoundedRectangle, {100, 50, 4}, fill: :blue %>

<%= component Examples.Component.ButtonIcon, "", translate: {10, 20} do
    primitive Scenic.Primitive.Text, @state.text, translate: {50, 0}
    primitive Scenic.Primitive.Text, @state.icon, translate: {0, 0}
end %>

<%= primitive Scenic.Primitive.RoundedRectangle, {100, 50, 4}, id: :btn, fill: :clear, input: true %>